{
  "name": "nanocal",
  "version": "0.4.0",
  "module": "dist/nanocal.min.js",
  "main": "dist/nanocal.min.js",
  "unpkg": "dist/nanocal.min.js",
  "repository": "https://github.com/zigomir/nanocal/tree/master/nanocal",
  "license": "MIT",
  "homepage": "https://github.com/zigomir/nanocal",
  "bugs": "https://github.com/zigomir/nanocal/issues",
  "description": "Minimal, AirBnb inspired, date picker",
  "scripts": {
    "dev": "tsc -w & npm run dev:nanocal & servor . index.html --no-browser --no-reload",
    "dev:nanocal": "rollup src/main.js -c rollup.config.js -w -f es -o dist/nanocal.js",
    "prebuild": "rm -f dist/*",
    "build": "tsc && npm run build:svelte && bundlesize",
    "build:svelte": "rollup src/main.js -c rollup.config.js -f es -o dist/nanocal.min.js",
    "prepublishOnly": "npm run build"
  },
  "bundlesize": [
    {
      "path": "dist/nanocal.min.js",
      "maxSize": "4.5 kB"
    }
  ],
  "dependencies": {
    "cntdys": "^0.2.8"
  },
  "devDependencies": {
    "bundlesize": "^0.17.1",
    "rollup": "^1.10.1",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "servor": "^2.2.1",
    "svelte": "^3.1.0",
    "typescript": "^3.4.5"
  }
}
